{
    "name": "admetrics/magento-module",
    "description": "Admetrics Data Studio Integration for Magento 2",
    "type": "magento2-module",
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": "^8.1",
        "magento/framework": "^103.0",
        "magento/module-store": "^101.1",
        "magento/module-catalog": "^104.0",
        "magento/module-checkout": "^100.4"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Admetrics\\Magento\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true
        }
    }
}
